#!/bin/bash
# export MD_NOTEBOOKS_DIR in your bashrc or zshrc to the directory you cloned md-notebooks
# Usage: tn {tool}
# example: tn bash
TOOL=${1}
if [[ -z ${TOOL} ]]; then
  TOOL=`ls $MD_NOTEBOOKS_DIR/Tools_Notes | fzf`
fi
  
echo $TOOL
mdcat -p "$MD_NOTEBOOKS_DIR/Tools_Notes/$TOOL/"$(ls "$MD_NOTEBOOKS_DIR/Tools_Notes/$TOOL" | fzf )
